{
	"name": "jsolly-website",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev",
		"build": "astro build && node process-html.mjs",
		"preview": "astro preview",
		"prepare": "if [ -z \"$CI\" ]; then husky; fi",
		"lint": "biome lint --no-errors-on-unmatched",
		"format": "biome format --no-errors-on-unmatched --write",
		"check:ts": "tsc --noEmit",
		"check:fix": "biome check --write --no-errors-on-unmatched",
		"check:changed:fix": "biome check --changed --no-errors-on-unmatched --write",
		"fix": "npm run check:fix && npm run check:ts",
		"outdated": "npm outdated",
		"update": "npx npm-check-updates -u && npm install"
	},
	"dependencies": {},
	"devDependencies": {
		"@fontsource/inter": "^5.2.6",
		"@fontsource/jetbrains-mono": "^5.2.6",
		"@fontsource/poppins": "^5.2.6",
		"@tailwindcss/vite": "^4.1.11",
		"sharp": "^0.34.3",
		"astro": "^5.11.1",
		"@astrojs/sitemap": "^3.4.1",
		"@astrojs/check": "^0.9.4",
		"@astrojs/ts-plugin": "^1.10.4",
		"@biomejs/biome": "2.0.6",
		"@tailwindcss/typography": "^0.5.16",
		"constructs": "^10.4.2",
		"globby": "^14.1.0",
		"html-minifier": "^4.0.0",
		"husky": "^9.1.7",
		"jsdom": "^26.1.0",
		"mdast-util-to-string": "^4.0.0",
		"reading-time": "^1.5.0",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.8.3",
		"unist-util-visit": "^5.0.0"
	}
}
